<div class="paginationMeta">
	<div id="per_page"><%= @per_page %></div>
	<div id="total"><%= @total %></div>
	<div id="pages"><%= @pages %></div>
	<div id="page"><%= @page %></div>
</div>

<div class="photoSet container">
	<% @photos.each_with_index do |photo,index| %>
	<div class="photoItem left">
		<%= image_tag photo.thumb, :title=>photo['title'], :alt=>photo.full('o'), :class=>'photo' %>
	</div>
	<% end %>
</div>